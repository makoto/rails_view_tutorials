<h1>Listing posts</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th>Published</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

{{#posts}}
  <tr>
    <td>{{title}}</td>
    <td>{{body}}</td>
    <td>{{published}}</td>
    <td><a href="/posts/{{id}}">Show</a></td>
    <td><a href="/posts/{{id}}/edit">Edit</a></td>
    <td><a href="/posts/{{id}}" data-confirm="Are you sure?" data-method="delete" rel="nofollow">Destroy</a></td>
  </tr>
{{/posts}}
</table>

